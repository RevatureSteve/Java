package com.revature.equals;

public class EqualsOpVsEqualsMethod {

	public static void main(String[] args) {

		/*
		 *  == works fine for primitives
		 */
		int x = 5;
		int y = 5;
		int z = 6;
		
		if( x == y){
			System.out.println( "x == y");
		}else{
			System.out.println( " x != y");
		}
		
		if( x == z){
			System.out.println( "x == z");
		}else{
			System.out.println( " x != z");
		}
		
		
		String str1 = "dog";
		String str2 = "dog";
		String str3 = "cat";
		String str4 = new String("cat");
		
				
	}

}
