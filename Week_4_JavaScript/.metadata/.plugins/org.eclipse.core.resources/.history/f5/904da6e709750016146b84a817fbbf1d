//Global Variables
var a, b, c, d, e, f, g, h, i;

a = 10;
b = "10";
c = true;
d = {};
e = null;
//f
g = (0/0);
h = [];
i = function(){};

//--------------------Strings are immutable
var myString = 'hello';
function changeString(myVar){
	console.log(myVar);
	myVar = "changed that string";
	console.log(myVar);
}


